{
  "name": "@minimizelab/mono",
  "version": "0.0.0",
  "description": "minimize mono repo",
  "repository": "git@github.com:minimizelab/mono.git",
  "author": "minimize <hello@minimize.se>",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx}\""
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "1.1.3"
  },
  "engines": {
    "node": "14.x"
  },
  "packageManager": "yarn@1.22.17"
}
